generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model User {
  id                  Int    @default(autoincrement()) @id
  name                String
  email               String @unique
  cpf                 String @unique
  escolaridade        String
  created_at          DateTime @default(now())
}
